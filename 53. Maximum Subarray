class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int s=nums.size();
        if(s==0) return 0;
        if(s==1) return nums[0];
        
        int result=INT_MIN;
        for(int i=0; i<s; i++){
            int temp=0;
            for(int j=i; j<s; j++){
                temp+=nums[j];
                result=max(result,temp);
            }
        }
        return result;
    }
};
